  "shapeRegionName": "GID_1",
  "outDirPath": "{CER_EXPLORE}/maps/image/World",
  "outFileGlue": "{dataSetName}_{dataVariableName}_squish_{dataTimeValue}.png",
  "widthInPx": 1920,
  "heightInPx": 1080,
  "dpi": 150
