{
  "_class": ["AggregateAllTimeConcat", "Run", "Opts"],
  "slurm": {
    "_class": ["Slurm", "Opts"],
    "prefix": "run",
    "qos": "standby",
    "cpusPerTask": 2,
    "timeInMinutes": 5,
    "mail": true,
    "logDirPath": "{CER_RUN}/_log",
    "nJobs": 1
  },
  "dirPath": "{CER_PROCESSED}/gswp3-w5e5-quantiles",
  "pattern": "quantilesTas_\\d+\\.nc",
  "outFilePath": "{CER_PROCESSED}/gswp3-w5e5-quantiles/quantilesTas.nc"
}
